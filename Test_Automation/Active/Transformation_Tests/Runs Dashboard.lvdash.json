  "datasets": [
    {
      "name": "76ed896b",
      "displayName": "runs",
      "queryLines": [
        "SELECT\r\n",
        "    r.run_id,\r\n",
        "    r.run_user,\r\n",
        "    r.run_start_datetime,\r\n",
        "    r.run_end_datetime,\r\n",
        "    r.run_status,\r\n",
        "    res.test_name,\r\n",
        "    res.status,\r\n",
        "    res.message\r\n",
        "FROM test_automation_runs r\r\n",
        "INNER JOIN test_automation_results res\r\n",
        "    ON r.run_id = res.run_id\r\n",
        "ORDER BY r.run_start_datetime DESC;\r\n"
      ]
    }
  ],
